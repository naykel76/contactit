{
    "name": "naykel/contactit",
    "description": "Simple contact package for NayKel Laravel Applications",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://github.com/naykel76/contactit.git",
    "keywords": ["Laravel", "Contactit"],
    "require": {
        "illuminate/support": "~7|~8",
        "spatie/laravel-honeypot": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Contactit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Contactit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Contactit\\ContactitServiceProvider"
            ]
        }
    }
}
